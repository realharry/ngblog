{
  "schema-version": 1,
  "site-info": {
    "name": "My Blog",
    "title": "NgBlog demo blog"
  },
  "contact-info": {
    "email": "ngblog@example.com",
    "phone": "555-555-5555",
    "website": "https://gitlab.com/ngblog"
  },
  "enable-weekly-digest": true,
  "enable-monthly-digest": true,
  "show-contact-email": true,
  "blog-post-folder": "posts-dev/",
  "max-post-age": 120,
  "old-post-list": ["20171231", "20171225"],
  "item-count-per-page": 20,
  "content-formats": ["markdown"],
  "use-hash-link-redirect": true,
  "accordion-ui-stale-age": 3600,
  "show-placeholder-thumbnail": false,
  "show-share-via-email": true,
  "show-share-on-twitter": true,
  "show-share-on-facebook": true,
  "show-share-on-linkedin": true,
  "enable-disqus-comment": true,
  "disqus-author-shortname": "ngblog-dev",
  "admin-visitor-token": "123456",
  "show-admin-button": true,
  "max-sitemap-age": 180,
  "ssr-enabled": false
}
